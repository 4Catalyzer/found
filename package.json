{
  "name": "found",
  "version": "0.3.14",
  "description": "Extensible route-based routing for React applications",
  "files": [
    "lib",
    "index.d.ts"
  ],
  "main": "lib/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "rimraf lib && babel src -d lib",
    "format": "eslint --fix examples/*/src src test && prettier --write '{src,test}/**/*.{ts,tsx}'",
    "lint": "eslint examples/*/src src test && prettier -l '{src,test}/**/*.{ts,tsx}'",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "tdd": "jest --watch",
    "test": "npm run lint && npm run tsc && npm run testonly -- --coverage",
    "testonly": "jest --runInBand --verbose",
    "tsc": "tsc -p tsconfig.json"
  },
  "lint-staged": {
    "globOptions": {
      "matchBase": false
    },
    "linters": {
      "**/*.js": [
        "eslint --fix",
        "git add"
      ],
      "**/*.{ts,tsx}": [
        "prettier --write",
        "git add"
      ],
      "README.md": [
        "doctoc"
      ]
    }
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**"
    ],
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4Catalyzer/found.git"
  },
  "keywords": [
    "react",
    "router",
    "routes",
    "routing"
  ],
  "author": "4Catalyzer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/4Catalyzer/found/issues"
  },
  "homepage": "https://github.com/4Catalyzer/found#readme",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "farce": "^0.2.6",
    "is-promise": "^2.1.0",
    "lodash": "^4.17.5",
    "path-to-regexp": "^1.7.0",
    "prop-types": "^15.6.1",
    "prop-types-extra": "^1.0.1",
    "react-redux": "^5.0.7",
    "react-static-container": "^1.0.2",
    "redux": "^4.0.0",
    "warning": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^0.14.9 || >=15.3.0"
  },
  "devDependencies": {
    "@types/react": "^16.3.11",
    "@types/react-dom": "^16.0.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "codecov": "^3.0.0",
    "delay": "^3.0.0",
    "doctoc": "^1.3.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-4catalyzer-react": "^0.4.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "p-defer": "^1.0.0",
    "prettier": "^1.12.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-proxy": "^3.0.0-alpha.1",
    "react-stand-in": "^4.0.0-beta.14",
    "react-test-renderer": "^16.3.2",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.3"
  }
}
