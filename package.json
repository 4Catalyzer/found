{
  "name": "found",
  "version": "2.0.0-rc.7",
  "description": "Extensible route-based routing for React applications",
  "keywords": [
    "react",
    "router",
    "routes",
    "routing"
  ],
  "homepage": "https://github.com/4Catalyzer/found#readme",
  "bugs": {
    "url": "https://github.com/4Catalyzer/found/issues"
  },
  "license": "MIT",
  "author": "4Catalyzer",
  "files": [
    "lib",
    "cjs"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/4Catalyzer/found.git"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "./build.sh",
    "format": "prettier --write .",
    "lint": "eslint src test",
    "prepublishOnly": "yarn build",
    "publishLocal": "yarn prepublishOnly && yarn file-butler prepare-publish-dir lib && yalc push lib",
    "release": "rollout --conventional-commits",
    "tdd": "vitest watch",
    "test": "yarn lint && yarn testonly",
    "testonly": "vitest run --coverage",
    "docs": "yarn --cwd www start"
  },
  "prettier": "@4c/prettier-config",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./cjs/index.d.ts",
        "default": "./cjs/index.js"
      },
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./cjs/*.d.ts",
        "default": "./cjs/*.js"
      },
      "import": {
        "types": "./lib/*.d.ts",
        "default": "./lib/*.js"
      }
    }
  },
  "dependencies": {
    "@restart/hooks": "^0.6.2",
    "dequal": "^2.0.3",
    "farce": "^0.4.5",
    "is-promise": "^4.0.0",
    "redux": "^4.2.0",
    "tiny-set-immediate": "^1.0.2",
    "tiny-warning": "^1.0.3"
  },
  "devDependencies": {
    "@4c/prettier-config": "^1.1.0",
    "@4c/rollout": "patch:@4c/rollout@npm%3A4.0.2#~/.yarn/patches/@4c-rollout-npm-4.0.2-ab2b6d0bab.patch",
    "@babel/cli": "^7.27.1",
    "@babel/core": "^7.27.1",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitest/coverage-v8": "3.1.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-dev-expression": "^0.2.3",
    "cherry-pick": "^0.5.0",
    "codecov": "^3.8.3",
    "delay": "^6.0.0",
    "doctoc": "^2.2.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.3.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "p-defer": "^4.0.1",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-test-renderer": "^19.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vitest": "^3.1.2",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "packageManager": "yarn@4.9.1+sha512.f95ce356460e05be48d66401c1ae64ef84d163dd689964962c6888a9810865e39097a5e9de748876c2e0bf89b232d583c33982773e9903ae7a76257270986538"
}
